{
  "name": "jhaemin.com-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel gen next:dev",
    "next:dev": "next dev --port 0311",
    "gen": "npm-run-all gen:apipaths",
    "gen:apipaths": "node scripts/generate-api-paths.mjs",
    "build": "npm run typecheck && next build",
    "start": "next start --port 7000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "pm2:start": "if pm2 show jhaemin.com ; then npm run pm2:stop && npm run pm2:delete && npm run pm2:start ; else pm2 start npm --name 'jhaemin.com' -- run start ; fi",
    "pm2:restart": "if pm2 show jhaemin.com ; then pm2 restart jhaemin.com ; else npm run pm2:start ; fi",
    "pm2:stop": "if pm2 show jhaemin.com ; then pm2 stop jhaemin.com ; else echo 'No pm2 instance for jhaemin.com client' ; fi",
    "pm2:delete": "if pm2 show jhaemin.com ; then pm2 delete jhaemin.com ; else echo 'No pm2 instance for jhaemin.com client' ; fi",
    "update": "git reset --hard && git pull && rm -rf node_modules && npm install && npm run lint && next build && npm run pm2:restart",
    "dbpush": "prisma db push",
    "introgen": "prisma introspect && prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "axios": "^0.23.0",
    "change-case": "^4.1.2",
    "clsx": "^1.1.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.10.7",
    "framework7-icons-plus": "^1.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "marked": "^3.0.7",
    "nanoid": "^3.1.30",
    "next": "11.1.2",
    "next-iron-session": "^4.2.0",
    "nodemailer": "^6.7.0",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^3.1.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.176",
    "@types/marked": "^3.0.2",
    "@types/nodemailer": "^6.4.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.31",
    "autoprefixer": "^10.3.7",
    "babel-plugin-superjson-next": "^0.4.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "prisma": "^3.3.0",
    "prismap": "^1.2.0",
    "sass": "^1.43.3",
    "superjson": "^1.8.0",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.4.4"
  }
}
